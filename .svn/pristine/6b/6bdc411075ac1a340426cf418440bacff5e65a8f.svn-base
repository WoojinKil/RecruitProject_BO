@charset "UTF-8";

/* 메인 배너 */
#mainBanner{position: relative;}
#mainBanner .logo{z-index: 10; position: absolute; left: 50%; top: 20px; -webkit-transform: translateX(-50%); transform: translateX(-50%); width: 185px; height: 30px; background: url("../images/img_m_logo.png") no-repeat 0 0; background-size: 100% auto; text-align: center;}
#mainBanner .logo a{font-size: 0;}
#mainBanner .main-swiper1 .slide{height: 250px;}
#mainBanner .main-swiper1 .slide .swiper-slide{position: relative; background: url(../images/img_main_slide_1.jpg) no-repeat 50% 50%; background-size: cover; text-align: center;}
#mainBanner .main-swiper1 .slide .swiper-slide img{width: 100%;}
#mainBanner .main-swiper1 .slide .swiper-slide .slide-text{position: absolute; left: 50%; top: 50%; -webkit-transform: translate(-50%, -50%); transform: translate(-50%, -50%); width: 100%;}
#mainBanner .main-swiper1 .slide .swiper-slide .slide-text p{font-size: 33px; line-height: 40px; font-weight: 300; letter-spacing: -0.75px; color: #fff;}
#mainBanner .swiper-pagination1{ z-index: 1; position: absolute; left: 0; bottom: 12px; width: 100%; text-align: center;}
#mainBanner .swiper-pagination-bullet{margin-left: 9px; background: #909090;}
#mainBanner .swiper-pagination-bullet:first-child{margin-left: 0;}
#mainBanner .swiper-pagination-bullet-active{background: #fff;}

/* 비즈니스IT 소개 */
#content .titleWrap{margin-top: 32px; padding: 0 26px;}
#content .titleWrap .title{font-size: 23px; line-height: 25px; font-weight: 300; letter-spacing: -0.5px; color: #333; text-align: center;}
#content .titleWrap .title em{font-weight: 500;}
#content .mainIntroduce .descWrap{margin-top: 59px; background: #f5f5f5;}
#content .mainIntroduce .descWrap .descInner{padding: 0 26px 31px;}
#content .mainIntroduce .descWrap .descInner .imgWrap{text-align: center;}
#content .mainIntroduce .descWrap .descInner .imgWrap img{margin-top: -35px; width: 100%; max-width: 600px;}
#content .mainIntroduce .descWrap .descInner .desc{margin-top: 21px;}
#content .mainIntroduce .descWrap .descInner .desc p{font-size: 13px; line-height: 21px; font-weight: 300; letter-spacing: -1px; color: #333; text-align: center;}

/* 비즈니스 IT 강의안내 */
#content .main-swiper2{margin-top: 25px; padding: 0 0 35px 15px;}
#content .main-swiper2 .couseList{font-size: 0; text-align: center;}
#content .main-swiper2 .swiper-slide{display: inline-block; vertical-align: top; margin-left: 15px; padding: 32px 25px 25px; width: 219px !important; height: 214px !important; -webkit-box-sizing: border-box; box-sizing: border-box;}
#content .main-swiper2 .swiper-slide:first-child{margin-left: 0;}
#content .main-swiper2 .swiper-slide.typeBlue{background: url("../images/bg_course_1.jpg") no-repeat 0 0; background-size: cover;}
#content .main-swiper2 .swiper-slide.typePurple{background: url("../images/bg_course_2.jpg") no-repeat 0 0; background-size: cover;}
#content .main-swiper2 .swiper-slide.typeDarkBlue{background: url("../images/bg_course_3.jpg") no-repeat 0 0; background-size: cover;}
#content .main-swiper2 .swiper-slide .textWrap{text-align: left;}
#content .main-swiper2 .swiper-slide .title{height: 44px; font-size: 18px; line-height: 22px; font-weight: 700; letter-spacing: 0; color: #fff; overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical;}
#content .main-swiper2 .swiper-slide .desc{margin-top: 9px; height: 48px; font-size: 11px; line-height: 16px; font-weight: 300; letter-spacing: 0; color: #eee; overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 3; -webkit-box-orient: vertical;}
#content .main-swiper2 .swiper-slide .infoList{margin-top: 12px;}
#content .main-swiper2 .swiper-slide .infoList li{margin-top: 4px; font-size: 12px; line-height: 14px; font-weight: 400; letter-spacing: 0; color: #fff;}
#content .main-swiper2 .swiper-slide .infoList li:first-child{margin-top: 0;}
#content .main-swiper2 .swiper-slide .infoList li span{display: inline-block; vertical-align: top;}
#content .main-swiper2 .swiper-slide .infoList li .square{margin-right: 7px; padding: 0 7px; height: 17px; line-height: 17px; border: 1px solid #fff; font-weight: 500;}
#content .main-swiper2 .swiper-slide .infoList li .infoText{height: 19px; line-height: 19px;}

/* 푸터 */
#footer{text-align: center;}
#footer .footerTop{background: #424242;}
#footer .footerTop .footerInner{padding: 0 26px 29px;}
#footer .footerTop .innerTitle{padding-top: 32px; font-size: 20px; line-height: 22px; font-weight: 300; letter-spacing: -0.5px; color: #999;}
#footer .footerTop .innerTitle.typeLocation{padding-top: 27px;}
#footer .footerTop .consultTel{margin-top: 13px; font-size: 30px; line-height: 32px; font-weight: 700; letter-spacing: 0; color: #fff;}
#footer .footerTop .consultTxt{margin-top: 12px; font-size: 11px; line-height: 18px; font-weight: 400; letter-spacing: 0; color: #fff;}
#footer .footerTop .location{margin-top: 18px;}
#footer .footerTop .location iframe{max-width: 600px;}
#footer .footerTop .location p{margin-top: 15px; font-size: 11px; line-height: 13px; font-weight: 300; letter-spacing: 0; color: #fff;}
#footer .footerTop .locationTxt{margin-top: 14px;}
#footer .footerTop .locationTxt .info li{font-size: 11px; line-height: 18px; font-weight: 300; letter-spacing: 0; color: #fff;}
#footer .footerTop .locationTxt .bus{margin-top: 14px; font-size: 11px; line-height: 18px; font-weight: 300; letter-spacing: 0; color: #aaa;}
#footer .footerTop .locationTxt .busInfo li{font-size: 11px; line-height: 18px; font-weight: 300; letter-spacing: 0; color: #fff;}  
#footer .footerTop .locationTxt .busInfo li .busText{position: relative; padding-left: 10px;}
#footer .footerTop .locationTxt .busInfo li .busText:before{content: ""; display: inline-block; position: absolute; left: 0; top: 6px; width: 5px; height: 5px; border-radius: 50%;}
#footer .footerTop .locationTxt .busInfo li.typeCyan .busText:before{background: #46d2d4;}
#footer .footerTop .locationTxt .busInfo li.typeBlue .busText:before{background: #4f7bfb;}
#footer .footerTop .locationTxt .busInfo li.typeGreen .busText:before{background: #4dcd59;}
#footer .footerTop .locationTxt .busInfo li.typeRed .busText:before{background: #eb3d49;}