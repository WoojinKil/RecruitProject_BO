	top: -2px;
	font-size: 15px;
	width: auto;
}

.tableTop {
	padding: 15px 0 5px;
	position: relative;
}

.tableTop.first {
	padding: 0 0 5px;
}

.tableTop:after {
	content: "";
	display: block;
	clear: both;
}

.tableTop .title {
	float: left;
	margin-right: 10px;
	font-size: 14px;
	color: #333333;
	font-weight: 400;
	height: 23px;
	line-height: 23px;
	font-family: 'Noto Sans KR', 'Dotum', '돋움', sans-serif;
}
/* 헤더 */
