}

.select {
	height: 23px;
	line-height: 23px;
	padding: 0 22px 0 5px;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	color: #666;
	font-size: 12px;
	font-weight: 300;
	letter-spacing: -0.5px;
	background: #fff
		url('../../pandora3/images/common_new/img_bottom_arrow.png') no-repeat
